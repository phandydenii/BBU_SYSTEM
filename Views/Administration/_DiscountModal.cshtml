<div class="modal fade" id="DiscountModal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Discount</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-5">
                        <div class="form-group">
                            <label>From Date:</label>
                            <input type="text" id="dtpFrom" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-5">
                        <div class="form-group">
                            <label>To Date:</label>
                            <input type="text" id="dtpTo" class="form-control">
                        </div>
                    </div>
                    <div class="col-md-2">
                        <div class="form-group">
                            <label>#</label>
                            <button class="form-control btn btn-success btn-sm">Save</button>
                        </div>
                    </div>
                </div>
                <table id="tblDiscountDetail" class="table table-bordered table-striped" style="width: 100%;">
                    <thead>
                        <tr>
                            <th>Student Id</th> 
                            <th>Gender</th>
                            <th>DOB</th> 
                        </tr>
                    </thead>
                    <tbody> 
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>